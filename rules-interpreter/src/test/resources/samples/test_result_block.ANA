workflow 'create-order'
    ruleset 'ok'
        'good_store' store_type = 'gym' return allow
        'good_user' user_id = '401912' return allow
    ruleset 'bad'
        'bad_user' user_id = '409012' return block
    ruleset 'remedy'
        'warning_user' user_id = '409012' return prevent
    default allow
end